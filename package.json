{
  "name": "wrapper-app",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "prestart": "load-env",
    "start": "expo start",
    "android": "expo start --android",
    "bundle:android": "bundletool build-apks --bundle=./output/wrapper.aab --output=./output/wrapper.apks --ks ./release.keystore --ks-key-alias sandm --mode=universal && unzip ./output/wrapper.apks -d apks && mv apks/universal.apk ./wrapper.apk && rm -rf apks && rm -rf output",
    "ios": "expo start --ios",
    "web": "expo start --web"
  },
  "dependencies": {
    "@babel/runtime": "^7.27.3",
    "@expo/vector-icons": "^14.1.0",
    "@react-navigation/native": "^7.1.10",
    "@react-navigation/native-stack": "^7.3.14",
    "@tanstack/react-query": "^5.80.3",
    "axios": "^1.9.0",
    "clsx": "^2.1.1",
    "expo": "~53.0.9",
    "expo-localization": "~16.1.5",
    "expo-status-bar": "~2.2.3",
    "nativewind": "^4.1.23",
    "react": "19.0.0",
    "react-native": "0.79.2",
    "react-native-reanimated": "3.17.4",
    "react-native-safe-area-context": "^5.4.0",
    "react-native-screens": "~4.10.0",
    "react-native-webview": "13.13.5",
    "zod": "^3.25.51"
  },
  "devDependencies": {
    "@babel/core": "^7.27.3",
    "@babel/plugin-transform-runtime": "^7.27.3",
    "@babel/preset-env": "^7.27.2",
    "@types/react": "~19.0.10",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.3"
  },
  "private": true
