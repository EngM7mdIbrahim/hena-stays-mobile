{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "baseUrl": ".",
    "paths": {
      "@apis": ["apis/index"],
      "@apis/*": ["apis/*"],
      "@commonTypes": ["types"],
      "@commonTypes/*": ["types/*"],
      "@hooks": ["hooks/index"],
      "@hooks/*": ["hooks/*"],
      "@config": ["config/index"],
      "@config/*": ["config/*"],
      "@interfaces": ["interfaces/index"],
      "@interfaces/*": ["interfaces/*"],
      "@utils": ["utils/index"],
      "@utils/*": ["utils/*"],
      "@assets/*": ["assets/*"],
      "@screens/*": ["screens/*"],
      "@layouts/*": ["layouts/*"],
      "@navigation/*": ["navigation/*"],
      "@components/*": ["components/*"],
      "@constants/*": ["constants/*"],
      "@constants": ["constants/index"],
      "@enums": ["enums/index"],
      "@enums/*": ["enums/*"],
      "@schemas/*": ["schemas/*"],
      "@schemas": ["schemas/index"],
    }
  }
}
